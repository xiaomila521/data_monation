{
  "rewrites": [
    {
      "source": "/mt/(.*)",
      "destination": "/api/proxy?target=mt&path=$1"
    },
    {
      "source": "/imt/(.*)",
      "destination": "/api/proxy?target=imt&path=$1"
    },
    {
      "source": "/ele/(.*)",
      "destination": "/api/proxy?target=ele&path=$1"
    },
    {
      "source": "/ielealsc/(.*)",
      "destination": "/api/proxy?target=ielealsc&path=$1"
    }
  ]
}
